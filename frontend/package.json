{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --write .",
    "e2e:init": "npx cypress install",
    "e2e:open": "npx cypress open --e2e",
    "e2e:run": "npx cypress run --e2e",
    "e2e:serve-run:ci": "npx -y start-server-and-test \"npm run dev -- --port 5173 --strictPort\" http://localhost:5173 \"npm run e2e:run\"",
    "e2e:serve-run:gui": "npx -y start-server-and-test \"npm run dev -- --port 5173 --strictPort\" http://localhost:5173 \"npm run e2e:open\""
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "langchain": "^0.3.30",
    "pinia": "^2.1.3",
    "qs": "^6.14.0",
    "vite-plugin-express": "^1.0.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@vitejs/plugin-vue": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.3",
    "vite": "^5.0.11"
  }
}
