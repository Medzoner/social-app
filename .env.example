# Configuration de la base de données
POSTGRES_DB=socialdb
POSTGRES_USER=socialuser
POSTGRES_PASSWORD=changeme_in_production
DB_PORT=5432

# Configuration Redis
REDIS_PASSWORD=changeme_in_production
REDIS_PORT=6379

# Configuration JWT (CRITIQUE - À CHANGER EN PRODUCTION)
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production
JWT_EXP=15m
REFRESH_EXP=24h

# Configuration OAuth Google (CRITIQUE - À CONFIGURER)
SSO_GOOGLE_CLIENT_ID=your_google_client_id_here
SSO_GOOGLE_CLIENT_SECRET=your_google_client_secret_here
SSO_GOOGLE_REDIRECT_URL=http://localhost:3222/oauth/google/callback

# Configuration Email (CRITIQUE - À CONFIGURER)
MAILER_HOST=smtp.gmail.com
MAILER_AUTH_USER=your_email@gmail.com
MAILER_AUTH_PASS=your_app_password_here
MAILER_FROM=noreply@socialapp.com

# Configuration SMS (CRITIQUE - À CONFIGURER)
SMS_ACCOUNT_ID=your_twilio_account_id
SMS_AUTH_TOKEN=your_twilio_auth_token
SMS_FROM=+15005550006

# Configuration LLM
LLM_URL=http://medzoner-srv.lan:11434/api

# Configuration de l'application
GIN_MODE=release
DEBUG=false

# Configuration des ports
BACKEND_PORT=8080
FRONTEND_PORT=3000
NGINX_PORT=80
NGINX_SSL_PORT=443

# Configuration du monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001
GRAFANA_PASSWORD=admin

# Configuration des variables d'environnement frontend
VITE_API_URL=http://localhost:8080
VITE_APP_NAME=Social App 